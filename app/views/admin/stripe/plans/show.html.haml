%div#fission-stripe-plan.container.container-padding
  %div.panel.panel-default
    %div.panel-heading
      %h4= "Stripe plan #{@plan[:name]}"
    %div.panel.body
      %table.table
        - @plan.keys.each do |key|
          %tr
            %td.col-md-3
              %b= key.to_s.humanize
            %td= @plan[key]